#!/bin/bash

# Input file
input_file="input.txt"

# Output file
output_file="output.txt"

# Process the file
awk -F '"' '/namespace=/ {
    namespace = $2
    gsub("-", "_", namespace)
    print namespace
}' "$input_file" > "$output_file"

echo "Namespaces have been extracted and hyphens replaced with underscores. Check $output_file for results."
